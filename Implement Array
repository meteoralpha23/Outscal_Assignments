#include <bits/stdc++.h>
using namespace std;
int main()
{
  int arr[15];
  
  for(int i=0;i<15;i++)
  {
      if(i%2==0)
      {
          arr[i] = 2;
      }
      else
      {
          arr[i] = 3;
      }
  }
  
  cout<<"Printing array"<<endl;
  
  for(int i=0;i<15;i++)
  {
      cout<<arr[i]<<endl;
  }
}
